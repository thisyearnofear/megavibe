{
  "name": "megavibe",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:frontend": "cd frontend && yarn dev",
    "dev:backend": "cd backend && yarn dev",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "clean": "rm -rf node_modules frontend/node_modules backend/node_modules",
    "install:all": "yarn install && cd frontend && yarn install && cd ../backend && yarn install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "dotenv": "^16.3.1",
    "gsap": "^3.13.0"
  }
}
