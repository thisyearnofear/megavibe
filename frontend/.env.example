# Development Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# API Configuration
VITE_API_URL=http://localhost:3000
VITE_WS_URL=http://localhost:3000
VITE_ENVIRONMENT=development

# Dynamic.xyz Configuration
VITE_DYNAMIC_ENVIRONMENT_ID=your_dynamic_environment_id_here

# Mantle Network Configuration (Sepolia Testnet)
VITE_MANTLE_RPC_URL=https://rpc.sepolia.mantle.xyz
VITE_MANTLE_CHAIN_ID=5003
VITE_MANTLE_NETWORK_NAME=Mantle Sepolia

# Smart Contract Configuration
VITE_EVENT_CONTRACT_ADDRESS=your_event_contract_address_here
VITE_TIPPING_CONTRACT_ADDRESS=your_tipping_contract_address_here
VITE_BOUNTY_CONTRACT_ADDRESS=your_bounty_contract_address_here
VITE_FEE_RECIPIENT_ADDRESS=your_fee_recipient_address_here
VITE_PLATFORM_FEE_PERCENTAGE=5

# Web3 Social Configuration
VITE_NEYNAR_API_KEY=your_neynar_api_key_here
VITE_NEYNAR_CLIENT_ID=your_neynar_client_id_here
VITE_LENS_API_URL=https://api.lens.dev

# Development Features
VITE_DEBUG_MODE=true
